  "productName": "Bool",
  "version": "0.1.0",
  "identifier": "com.bmstu.bool",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://192.168.31.50:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Bool",
